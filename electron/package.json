{
  "name": "mizan",
  "version": "1.0.0",
  "description": "An Amazing Capacitor App",
  "main": "./build/src/index.js",
  "scripts": {
    "build": "tsc",
    "electron:start": "npm run build && electron ./",
    "electron:pack": "npm run build && electron-builder build --dir",
    "electron:build-windows": "npm run build && electron-builder build --windows",
    "electron:build-mac": "npm run build && electron-builder build --mac",
    "server": "nodemon --watch 'src/**' --ext 'ts, json, gql' --exec 'ts-node src/server'"
  },
  "dependencies": {
    "@capacitor-community/electron": "^1.3.1",
    "@thiagoelg/node-printer": "^0.5.3",
    "apollo-server": "^2.21.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "pouchdb": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "ramda": "^0.27.1",
    "serialport": "^9.0.6",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@serialport/parser-delimiter": "^9.0.1",
    "@types/express": "^4.17.11",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-find": "^6.3.6",
    "@types/serialport": "^8.0.1",
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "electron": "~10.3.1",
    "electron-builder": "^22.9.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "keywords": [
    "capacitor",
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "build": {
    "appId": "com.example.capacitor-app",
    "productName": "Capacitor App",
    "files": [
      "assets/*",
      "build/*",
      "preloader.js",
      "plugins/*",
      "capacitor.config.json",
      "app/**"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "appIcon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "appIcon.ico"
    }
  }
}
